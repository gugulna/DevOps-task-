replicaCount: 1
image:
  repository: <gcr.io/DevOps-task>/product-catalogue
  tag: latest
containerName: product-catalogue
containerPort: 8020
command: ["java", "-Djava.security.egd=file:/dev/./urandom", "-jar", "app.jar", "server"]
serviceType: ClusterIP
servicePort: 80
ingress:
  enabled: true
  host: example.com
  path: /product-catalogue
issuer:
  name: letsencrypt-prod
  kind: ClusterIssuer
